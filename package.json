{
  "name": "sassdoc-theme-herman",
  "title": "Herman",
  "description": "An Odd SassDoc theme.",
  "version": "5.0.0-beta.3",
  "homepage": "https://www.oddbird.net/herman/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oddbird/sassdoc-theme-herman.git"
  },
  "bugs": {
    "url": "https://github.com/oddbird/sassdoc-theme-herman/issues"
  },
  "keywords": [
    "sassdoc-theme"
  ],
  "files": [
    "index.js",
    "dist/",
    "!dist/webpack/app_styles.min.js",
    "!dist/webpack/iframe_styles.min.js",
    "!dist/webpack/styleguide_json*",
    "lib/",
    "templates/",
    "scss/utilities/",
    "scss/_utilities.scss",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "run-s build:sprites build:webpack",
    "serve": "run-s build:sprites serve:webpack",
    "lint": "run-p lint:js lint:sass prettier:other",
    "test": "run-s test:sass test:js",
    "serve:webpack": "webpack serve",
    "build:webpack": "webpack",
    "build:sprites": "node scripts/compile-sprites.js",
    "prettier:js": "prettier --write \"**/*.js\"",
    "prettier:other": "prettier --write \"**/*.{json,yml}\"",
    "eslint": "yarn lint:js:ci --fix",
    "lint:js": "run-s prettier:js eslint",
    "lint:js:ci": "eslint \"*.js\" \"**/*.js\"",
    "lint:sass": "yarn lint:sass:ci --fix",
    "lint:sass:ci": "stylelint \"**/*.scss\"",
    "lint:ci": "run-p lint:js:ci lint:sass:ci",
    "test:js": "run-s test:js:client test:js:src",
    "test:js:client": "jest",
    "test:js:src": "nyc mocha -R dot test/js",
    "test:js:client:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:js:src:ci": "nyc mocha -R dot -R mocha-junit-reporter test/js",
    "test:js:watch": "yarn test:js:client --watchAll",
    "test:sass": "mocha -R dot test/sass/test_sass.js",
    "postinstall": "yarn husky install",
    "prepack": "yarn pinst --disable && yarn build",
    "postpack": "yarn pinst --enable"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "cheerio": "1.0.0-rc.12",
    "colorjs.io": "0.4.1-patch.1",
    "extend": "^3.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-svg-symbols": "^3.2.3",
    "html": "^1.0.0",
    "lunr": "^2.3.9",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
    "nunjucks": "^3.2.3",
    "readable-stream": "^4.2.0",
    "sassdoc-extras": "^3.0.0",
    "strip-indent": "^3.0.0",
    "typogr": "^0.6.8",
    "unixify": "^1.0.0",
    "vinyl-fs": "^3.0.3"
  },
  "peerDependencies": {
    "sassdoc": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@yarnpkg/sdks": "^3.0.0-rc.27",
    "accoutrement": "^4.0.3",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.3.0",
    "babel-loader": "^9.1.0",
    "chalk": "^4.1.2",
    "core-js": "^3.26.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "cssremedy": "^0.1.0-beta.2",
    "del": "^6.1.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.4",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "highlight.js": "^11.6.0",
    "husky": "^8.0.1",
    "jest": "^29.3.0",
    "jest-environment-jsdom": "^29.3.0",
    "jest-junit": "^14.0.1",
    "jinja-loader": "^0.0.8",
    "jquery": "^3.6.1",
    "jquery-deparam": "^0.5.3",
    "js-yaml": "^4.1.0",
    "json-loader": "^0.5.7",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "mark.js": "^8.11.1",
    "matchmedia-polyfill": "^0.3.2",
    "mini-css-extract-plugin": "^2.6.1",
    "mocha": "^10.1.0",
    "mocha-junit-reporter": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "pinst": "^3.0.0",
    "postcss": "^8.4.18",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "sass": "^1.56.0",
    "sass-loader": "^13.1.0",
    "sass-true": "^6.1.0",
    "sassdoc": "^2.7.4",
    "sinon": "^14.0.2",
    "srcdoc-polyfill": "^1.0.0",
    "stylelint": "^14.14.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-prettier": "^2.0.0",
    "svgo": "^3.0.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "resolutions": {
    "cdocparser": "^0.15.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint --fix"
    ],
    "*.{json,yml}": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@3.2.4"
}
